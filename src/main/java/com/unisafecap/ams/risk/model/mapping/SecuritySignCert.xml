	</sql>
	
	<sql id="other-condition">
            <if test= "orgCode != null and orgCode != '' ">	and org_code = #{orgCode,jdbcType=VARCHAR }</if>
            <if test= "trustProjectCode != null and trustProjectCode != '' ">	and trust_project_code = #{trustProjectCode,jdbcType=VARCHAR }</if>
            <if test= "orgCodeCertPath != null and orgCodeCertPath != '' ">	and org_code_cert_path = #{orgCodeCertPath,jdbcType=VARCHAR }</if>
            <if test= "orgCodeCertPwd != null and orgCodeCertPwd != '' ">	and org_code_cert_pwd = #{orgCodeCertPwd,jdbcType=VARCHAR }</if>
